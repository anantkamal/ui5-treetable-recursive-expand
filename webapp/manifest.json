{
  "_version": "1.76.0",
  "sap.app": {
    "id": "openui5.treetable.recursiveexpand",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:basic",
      "version": "1.18.7",
      "toolsId": "236b2449-5b02-43ba-9831-429b01e5d753"
    },
    "dataSources": {
      "clothingDataSource": {
				"uri": "localData/Clothing.json",
				"type": "JSON"
		  }
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": true,
    "dependencies": {
      "minUI5Version": "1.140.0",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "openui5.treetable.recursiveexpand.i18n.i18n"
        }
      },
      "": {
				"type": "sap.ui.model.json.JSONModel",
				"dataSource": "clothingDataSource"
			}
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "controlAggregation": "pages",
        "controlId": "app",
        "transition": "slide",
        "type": "View",
        "viewType": "XML",
        "path": "openui5.treetable.recursiveexpand.view",
        "async": true,
        "viewPath": "openui5.treetable.recursiveexpand.view"
      },
      "routes": [
        {
          "name": "RouteCatalogHierarchy",
          "pattern": ":?query:",
          "target": [
            "TargetCatalogHierarchy"
          ]
        }
      ],
      "targets": {
        "TargetCatalogHierarchy": {
          "id": "CatalogHierarchy",
          "name": "CatalogHierarchy"
        }
      }
    },
    "rootView": {
      "viewName": "openui5.treetable.recursiveexpand.view.App",
      "type": "XML",
      "id": "App",
      "async": true
    }
  }
}
